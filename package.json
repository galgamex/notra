	"scripts": {
		"dev": "prisma generate && next dev --turbopack",
		"build": "prisma generate && prisma migrate deploy && next build",
		"start": "next start",
		"lint": "eslint . && prettier --check . && stylelint \"**/*.{css,scss}\" && pnpm check-types",
		"lint:fix": "eslint . --fix && prettier --write . && stylelint \"**/*.{css,scss}\" --fix",
		"prepare": "husky",
		"db:generate": "prisma generate",
		"db:push": "dotenv -e .env.local -- prisma db push",
		"db:migrate": "dotenv -e .env.local -- prisma migrate dev",
		"db:init-website": "dotenv -e .env.local -- tsx scripts/init-website-data.ts",
		"db:add-sample-clicks": "dotenv -e .env.local -- tsx scripts/add-sample-clicks.ts",
		"check-types": "tsc --noEmit"
	},
